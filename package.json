{
  "name": "array-join",
  "version": "2.4.0",
  "description": "Join (SQL-like) arrays of objects by a common key or with a custom match function.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iredchuk/array-join"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "author": {
    "name": "Igor Redchuk",
    "email": "redchuk@gmail.com"
  },
  "keywords": [
    "array",
    "arrays",
    "join",
    "left-join",
    "util"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "pretest": "xo",
    "test": "tsd && jest --coverage",
    "prepare": "npm test"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "tsd": "^0.7.4",
    "xo": "^0.24.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write **/*.js",
      "xo",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "xo": {
    "envs": [
      "node",
      "jest"
    ],
    "prettier": true
  }
}

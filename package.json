{
  "name": "array-join",
  "version": "2.5.0",
  "description": "Join (SQL-like) arrays of objects by a common key or with a custom match function.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iredchuk/array-join"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "author": {
    "name": "Igor Redchuk",
    "email": "redchuk@gmail.com"
  },
  "keywords": [
    "array",
    "arrays",
    "join",
    "left-join",
    "util"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "tsd && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "prepare": "npm test"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "tsd": "^0.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write **/*.js",
      "npm run lint"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
